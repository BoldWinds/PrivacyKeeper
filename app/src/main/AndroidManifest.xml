<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="privacykeeperv1">

    <uses-permission android:name="android.permission.USE_BIOMETRIC" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.lbw.privacykeeper.PrivacyKeeperApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_lock_fill0_wght400_grad0_opsz48"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_lock_fill0_wght400_grad0_opsz48"
        android:supportsRtl="true"
        android:theme="@style/Theme.App.SplashScreen">
        <activity
            android:name="com.lbw.privacykeeper.ui.MainActivity"
            android:theme="@style/Theme.App.SplashScreen"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>